{
    "[css]": {
        "editor.defaultFormatter": "aeschli.vscode-css-formatter"
    },
    "[markdown]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.markdownlint": "explicit"
        },
        "editor.defaultFormatter": "yzhang.markdown-all-in-one"
    },
    "markdownlint.config": {
        "MD033": false
    }
}
